{
  "version": "0.1.0",
  "configurations": [
      {
          "name": "Attach",
          "type": "chrome",
          "request": "attach",
          "port": 9222,
          "url": "localhost:3000",
          "webRoot": "${workspaceRoot}",
          "skipFiles": [
              "node_modules",
              "webpack"
          ]
      },
      {
          "name": "Attach to url with files served from ./out",
          "type": "chrome",
          "request": "attach",
          "port": 9222,
          "url": "<url of the open browser tab to connect to>",
          "webRoot": "${workspaceRoot}"
      }
  ]
}
